							"script": {
								"id": "fbaae93a-3117-4f8c-8f27-90ef7f78b0fa",
								"exec": [
									"let currentCount = pm.environment.get(\"userCount\");\r",
									"let savedUsersSha1m = pm.environment.get(\"usersSha1m\");\r",
									"\r",
									"if (currentCount > 1){\r",
